pipeline {
    agent any
    environment{
        APP_NAME= 'app-demo'
    }
    stages{
        stage('Install-Dependencies') {
            agent {
                docker {
                    image 'node:erbium-alpine'
                }           
            }
            steps {
                    sh 'npm --version'
            }
        }
        stage ('build'){
            steps{
                sh 'docker --version'
            }
        }
    }
}